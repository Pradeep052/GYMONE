
@model List<GYM.DAL.Domain.MemberRegistration>
@{
    ViewBag.Title = "Member Registration Details";
}
<script type="text/javascript">
    $(document).ready(function () {
        $('#example').DataTable();
    });
    </script>
<div class="container">
    <div class="clearfix"></div>
    <fieldset>
        <legend class="legend">Member Registration Details</legend>

        <div class="panel">

            <div class="panel-body">


                <div>
                    @Html.ActionLink("Create Member", "Create", "RegisterMember", new { @class = "btn btn-info btn-pressure" })
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <table id="example" class="table table-striped table-bordered" cellspacing="0" >
                            <thead>
                                <tr>
                                    <td>MemberNo</td>
                                    <td>FullName</td>
                                    <td>JoiningDate</td>
                                    <td>DOB</td>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.MemberNo</td>
                                            <td>@item.MemberFname</td>
                                            <td>@item.JoiningDate</td>
                                            <td>@item.Dob</td>
                                        </tr>

                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                </div>

            </div>
        </div>

    </fieldset>

</div>